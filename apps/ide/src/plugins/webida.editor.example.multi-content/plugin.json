{
    "name": "webida.editor.example.multi-content",
    "description": "Multi Content Editor Example",
    "version": "0.1.0",
    "requirement": "webida.common.editors",
    "extensions" : {

        "webida.common.editors:editor" : {
            "name" : "Multi Content Editor Example",
            "editorPart" : "MultiContentExampleEditorPart",
